
<table class="table table-striped" id="movies_table">
	<thead>
	<tr>
		<th><center>Poster</center></th>
		<th><center>Name</center></th>
	</tr>
			</thead>
			<tbody>
				<% for movie in movies %>
					<tr>
						<td><%= link_to(image_tag(movie.poster(:medium)), city_movies_path('lahore',movie))%></td>
						<td><%= link_to movie.name.titleize, city_movies_path('lahore',movie)%></td>
					</tr>
				<% end %>
			</tbody>
</table>
